{
    "name": "joaozica/laravel-mpdf",
    "description": "Laravel Mpdf: Using Mpdf in Laravel to generate Pdfs.",
    "keywords": ["mpdf", "pdf", "laravel"],
    "license": "MIT",
    "require": {
        "php": ">=7.0.0",
        "mpdf/mpdf": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Meneses\\LaravelMpdf\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Meneses\\LaravelMpdf\\LaravelMpdfServiceProvider"
            ],
            "aliases": {
                "PDF": "Meneses\\LaravelMpdf\\Facades\\LaravelMpdf"
            }
        }
    }
}
